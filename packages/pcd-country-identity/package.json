{
  "name": "pcd-country-identity",
  "version": "0.0.7",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "repository": "https://github.com/privacy-scaling-explorations/anon-aadhaar",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts",
    "dev-setup": "/bin/bash ./script/setup-dev.sh setup",
    "dev-install": "/bin/bash ./script/setup-dev.sh install",
    "test": "npx ts-mocha test/**/*.test.ts --exit",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "cd ../.. && husky install packages/pcd-country-identity/.husky",
    "gen-pdf": "/bin/bash ./script/setup-dev.sh pdf-setup",
    "test-publish": "yarn build && yarn lint && yarn test && npm publish --dry-run",
    "gen-contract": "/bin/bash ./script/setup-dev.sh contract-setup",
    "gen-proof": "/bin/bash ./script/setup-dev.sh gen-proof"
  },
  "files": [
    "./artifacts/verification_key.json",
    "./src/*",
    "./dist/*",
    "./README.md"
  ],
  "dependencies": {
    "@pcd/passport-ui": "^0.6.0",
    "@pcd/pcd-types": "^0.6.0",
    "@pcd/tsconfig": "^0.6.0",
    "axios": "^1.4.0",
    "json-bigint": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "snarkjs": "^0.7.0",
    "styled-components": "^6.0.0-rc.1",
    "uuid": "^9.0.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/json-bigint": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.1",
    "@types/valid-url": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "chai": "^4.3.7",
    "circomlib": "^2.0.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "mocha": "^10.2.0",
    "node-forge": "^1.3.1",
    "node-signpdf-gen": "^1.5.0",
    "pdfkit": "^0.10.0",
    "prettier": "^2.8.8",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.0.4"
  }
}
